{
  "name": "<%= projectNameSlugified %>",
  "title": "<%= projectName %>",
  "description": "<%= projectDescription %>",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "<%= repoUrl %>"
  },
  "authors": [
    {
      "name": "<%= devNames %>"
    }
  ],
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "grunt test",

    "prestart": "npm-run-all prune install",
    "start": "grunt start",

    "predeploy": "npm-run-all prune install --no-optional",
    "deploy": "grunt compile --release",

    "info": "npm-scripts-info"
  },
  "scripts-info": {
    "test": "Run tests",

    "prestart": "Install npm dependencies",
    "start": "Open starter checklist on trykickoff.com",

    "predeploy": "Install non-optional npm dependencies",
    "deploy": "Compile all assets for production deployment"
  },
  "dependencies": {
    "console": "^0.5.2",
    "lite-ready": "^1.0.4"<%

    if (!includeDominus || !includeJquery) {%>,
    "double-dollar": "^1.0.0"<%
    }

    if (includeLodash) {%>,
    "lodash": "^4.13.1"<%
    }

    if (includeLazysizes) {%>,
    "lazysizes": "^1.5.0"<%
    }

    if (includeAxios) {%>,
    "axios": "^0.11.1"<%
    }

    if (includeFlickity) {%>,
    "flickity": "^1.2.0"<%
    }

    if (includeAttach) {%>,
    "attach.js": "^1.0.5"<%
    }

    if (includeDominus) {%>,
    "dominus": "^5.0.6"<%
    }

    if (includeSwiftclick) {%>,
    "swiftclick": "^1.2.0"<%
    }

    if (includeTrak) {%>,
    "trak.js": "^0.5.2"<%
    }

    if (includeJquery && oldIE && !includeDominus) {%>,
    "jquery": "^1.11.1"<%
    }

    if (includeJquery && !oldIE && !includeDominus) {%>,
    "jquery": "^2.2.3"<%
    } %>
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.3.0",
    "cssnano": "^3.5.2",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-imagemin": "^1.0.0",
    "grunt-newer": "^1.2.0",
    "grunt-shimly": "^1.0.2",
    "imagemin-gifsicle": "4.2.0",
    "imagemin-mozjpeg": "5.1.0",
    "imagemin-optipng": "4.3.0",
    "imagemin-pngquant": "4.2.2",
    "imagemin-svgo": "4.2.1",
    "load-grunt-configs": "^1.0.0",
    "load-grunt-tasks": "^3.5.0",
    "minifyify": "^7.3.3",
    "postscss": "^1.4.1",
    "time-grunt": "^1.3.0"<%
    if (statix) {%>,
    "grunt-assemble": "^0.4.0",
    "handlebars-helper-aggregate": "^0.1.3"<% } %>
  },
  "optionalDependencies": {
    "browser-sync": "^2.12.8",
    "commitizen": "^2.8.1",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^2.10.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "grunt-browser-sync": "^2.1.3",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^18.1.0",
    "grunt-filesizegzip": "^0.3.0",
    "grunt-scss-lint": "^0.3.8",
    "npm-scripts-info": "^0.3.6",
    "opn": "^4.0.1"
  },
  "keywords": [],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
