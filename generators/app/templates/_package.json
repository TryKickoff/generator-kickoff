{
  "name": "<%= projectNameSlugified %>",
  "title": "<%= projectName %>",
  "description": "<%= projectDescription %>",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "<%= repoUrl %>"
  },
  "authors": [
    {
      "name": "<%= devNames %>"
    }
  ],
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "grunt test",

    "prestart": "npm-run-all prune install",
    "start": "grunt start",

    "predeploy": "npm-run-all prune install --no-optional",
    "deploy": "grunt compile --release",

    "info": "npm-scripts-info"
  },
  "scripts-info": {
    "test": "Run tests",

    "prestart": "Install npm dependencies",
    "start": "Open starter checklist on trykickoff.com",

    "predeploy": "Install non-optional npm dependencies",
    "deploy": "Compile all assets for production deployment"
  },
  "dependencies": {
    "console": "^0.5.2",
    "lite-ready": "^1.0.4"<%

    if (includeSwiftclick) {%>,
    "swiftclick": "^1.2.0"<%
    }

    if (includeTrak) {%>,
    "trak.js": "^0.5.2"<%
    }

    if (includeJquery && oldIE) {%>,
    "jquery": "^1.11.1"<%
    }

    if (includeJquery && !oldIE) {%>,
    "jquery": "^2.2.3"<%
    } %>
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babelify": "^7.3.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "cssnano": "^3.5.2",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-imagemin": "^1.0.0",
    "grunt-newer": "^1.2.0",
    "grunt-shimly": "^1.0.2",
    "imagemin-gifsicle": "^5.0.0",
    "imagemin-jpegtran": "^5.0.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-optipng": "^5.0.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.0.0",
    "load-grunt-configs": "^0.4.3",
    "load-grunt-tasks": "^3.5.0",
    "minifyify": "^7.3.3",
    "postscss": "^1.4.1"<%
    if (statix) {%>,
    "grunt-assemble": "^0.4.0",
    "handlebars-helper-aggregate": "^0.1.3"<% } %>
  },
  "optionalDependencies": {
    "browser-sync": "^2.12.5",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^2.0.0",
    "eslint-plugin-import": "^1.6.1",
    "grunt-browser-sync": "^2.1.3",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^18.1.0",
    "grunt-filesizegzip": "^0.2.0",
    "npm-scripts-info": "^0.3.6",
    "opn": "^4.0.1",
    "grunt-scss-lint": "^0.3.8"
  },
  "keywords": [],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign"
    ]
  }
}
